
import java.util.Scanner;
public class Main{
    public static void moveToLast(int[]arr,int target){
        int n=arr.length;
        int index=0;
        for(int i=0;i<n;i++){
            if(arr[i]!=target){
                arr[index++]=arr[i];
            }
        }
        while(index<n){
            arr[index++]=target;
        }
        }
        public static void printArray(int[]arr){
for(int num:arr){
    System.out.print(num+ " ");
}
System.out.println();
}
public static void main(String []args){
    Scanner sc=new Scanner(System.in);
    System.out.print("Enter size of array");
    int n=sc.nextInt();
    int[]arr=new int[n];
    System.out.println("Enter"+n+"element:");
    for(int i=0;i<n;i++){
        arr[i]=sc.nextInt();
    }
    System.out.print("Enter elementto move to last:");
    int target=sc.nextInt();
    moveToLast(arr,target);
    
    System.out.print("Array after moving"+target+"s to last:");
    printArray(arr);
    }
}
